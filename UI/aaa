        Settings.ChangeGlider = v
    end)
    Section54.CreateDropdown({Title = 'Select Glider', List = {"Parachute", "Glider", "DebugGlider"}, Default = Settings.Glider}, function(v)
        Settings.Glider = v
    end)
end
