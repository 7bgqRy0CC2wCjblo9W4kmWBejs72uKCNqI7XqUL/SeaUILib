function AutoUseDispenser()
    while Settings.AutoDispenser and wait(2) do 
        for i, v in pairs(Dispensers) do
            if CheckToyCD(v) then 
                game:GetService("ReplicatedStorage").Events.ToyEvent:FireServer(v)
            end
        end
    end
end
