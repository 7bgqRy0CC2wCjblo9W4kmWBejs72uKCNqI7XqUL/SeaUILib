function CheckNguyenLieu(ng) 
local ind = nll.Get(ng)
local k = false
for k,v in pairs(ind.Ingredients) do 
local t = GetItemListWithValue()
if t[v.Type] and (t[v.Type]>= v.Amount) then 
else
    k=true
    break;
end
end
if not k then return true else return false end
end
